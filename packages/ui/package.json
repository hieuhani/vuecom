{
  "name": "@vuecommerce/ui",
  "version": "0.0.1",
  "scripts": {
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/styles/ -o dist/css/",
    "build": "vue-cli-service build --target lib --dest dist/js ./src/index.ts && yarn css-compile",
    "dev": "vue-cli-service build --target lib --dest dist/js --watch ./src/index.ts",
    "storybook": "vue-cli-service storybook:serve"
  },
  "files": [
    "dist"
  ],
  "main": "dist/js/ui.common.js",
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.4.0"
  },
  "devDependencies": {
    "@storybook/vue": "^5.3.12",
    "@types/jest": "^25.1.2",
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-plugin-typescript": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-storybook": "^1.2.0",
    "vue-template-compiler": "^2.6.11"
  }
}
